{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "prettier",
        "import"
    ],
    "rules": {
        "max-len": "off",
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off",
        "quotes": "off",
        "quote-props": "off",
        "object-curly-spacing": "off"
    }
}